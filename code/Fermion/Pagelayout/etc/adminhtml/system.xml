                 <field id="email_list" translate="label comment" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>List of Email id</label>
                    <comment>Please enter comma seprated list of email id. </comment>
                </field>
            </group>
	    </section>        
    </system>
</config>
